type Option a = Some a | None

let map f = \x -> match x with
    Some y -> Some (f y),
    None -> None

let incr : Int -> Int
let incr = add 1

rec fact n =
    if eq n 0 then
        1
    else
        mul n (fact (sub n 1))

let x = Some 1

let incr_opt = map incr

let main = (fact 6, incr_opt x)